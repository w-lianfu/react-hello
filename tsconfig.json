{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "paths": {
      "@/*": ["src/*"],
      "@common/*": ["src/common/*"],
      "@commonComp/*": ["src/common/component/*"],
      "@commonStore/*": ["src/common/store/*"],
      "@commonScss/*": ["src/common/scss/*"],
      "@commonStyled/*": ["src/common/styled/*"],
      "@comp/*": ["src/component/*"],
      "@con/*": ["src/container/*"],
      "@store/*": ["src/store/*"],
      "@scss/*": ["src/scss/*"],
      "@styled/*": ["src/styled/*"],
      "@util/*": ["src/util/*"],
      "@tool/*": ["src/util/tool/*"],
      "@theme/*": ["src/util/theme/*"],
      "@lib/*": ["src/lib/*"],
      "@assets/*": ["src/assets/*"],
      "@image/*": ["src/assets/image/*"],
      "@doc/*": ["src/doc/*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "config",
    "public",
    "scripts"
  ]
}
